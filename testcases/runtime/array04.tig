let
    type vector = array of int
    type matrix = array of vector
	var a:matrix := matrix[2] of vector[2] of 1
in
    test(a[0][0] = 1);
    test(a[0][1] = 1);
    test(a[1][0] = 1);
    test(a[1][1] = 1);

	a[0][1] := 0;
    a[1][0] := 0;

    test(a[0][0] = 1);
    test(a[0][1] = 0);
    test(a[1][0] = 0);
    test(a[1][1] = 1);

    print("ok\n")
end
